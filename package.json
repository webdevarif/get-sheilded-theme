{
  "name": "get-sheilded-theme",
  "version": "1.0.0",
  "description": "Modern WordPress theme with OOP architecture, pure PHP admin, and Gutenberg blocks",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:gutenberg\" \"npm run dev:admin\"",
    "dev:frontend": "webpack --mode development --watch --config webpack.frontend.js",
    "dev:gutenberg": "webpack --mode development --watch --config webpack.gutenberg.js",
    "dev:admin": "webpack --mode development --watch --config webpack.admin.js",
    "build": "npm run build:frontend && npm run build:gutenberg && npm run build:admin",
    "build:frontend": "webpack --mode production --config webpack.frontend.js",
    "build:gutenberg": "webpack --mode production --config webpack.gutenberg.js",
    "build:admin": "webpack --mode production --config webpack.admin.js"
  },
  "dependencies": {
    "@babel/preset-react": "^7.27.1",
    "@wordpress/block-editor": "^15.3.0",
    "@wordpress/blocks": "^15.3.0",
    "@wordpress/components": "^30.3.0",
    "@wordpress/element": "^6.30.0",
    "@wordpress/i18n": "^6.3.0",
    "gsap": "^3.12.2",
    "lucide-react": "^0.542.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-select": "^5.10.2",
    "vanilla-tilt": "^1.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@types/node": "^20.10.0",
    "@wordpress/babel-preset-default": "^8.30.0",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.5.6",
    "postcss-loader": "^7.3.3",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "keywords": [
    "wordpress",
    "theme",
    "gutenberg",
    "gsap",
    "php"
  ],
  "author": "Your Name",
  "license": "GPL-2.0+"
}
